{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.14.4",
    "@auth0/auth0-react": "^1.3.0",
    "@umijs/preset-react": "1.x",
    "umi": "^3.3.9"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@umijs/test": "^3.3.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.2.2",
    "yorkie": "^2.0.0"
  }
}
